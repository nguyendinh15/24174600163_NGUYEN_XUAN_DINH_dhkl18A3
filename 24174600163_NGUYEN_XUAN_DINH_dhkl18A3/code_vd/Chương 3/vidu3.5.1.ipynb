    "import numpy as np \n",
    "#Mảng đích để lưu trữ kết quả \n",
    "output=np.zeros((3,3)) \n",
    "#Mảng nguồn arr1, arr2 \n",
    "arr1=np.array([1,2,3]) \n",
    "arr2=np.array([4,5,6]) \n",
    "#Thực hiện phép cộng và lưu trữ kết quả vào mảng đích output \n",
    "np.add(arr1,arr2, out=output) \n",
    "#In kết quả \n",
    "print(output)"
   ]
  }
